<section>
    <div class ="container">
        <div class="row mt-4">
        <a href="/admin/add-product" class="btn btn-success ml-auto">Add Product</a>
    <table class="table mt-5">
  <thead>
    <tr>
      <th scope="col">No</th>
      <th scope="col">Name</th>
      <th scope="col">Category</th>
      <th scope="col">Ram</th>
    </tr>
  </thead>
  <tbody>
      {{#each products}}
    <tr>
      <th scope="row ">-</th>
      <td>{{this.Name}}</td>
      <td>{{this.Category}}</td>
      <td>{{this.Price}}</td>
     <td><img style=" height:50px; width:50px"src="/product-images/{{this._id}}.jpg"></td>
     
    </tr>
    {{/each}}
  
  </tbody>
</table>
  </div>

</section>